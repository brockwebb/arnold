<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.arnold.sync-daily</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/opt/anaconda3/envs/arnold/bin/python</string>
        <string>/Users/brock/Documents/GitHub/arnold/scripts/sync_pipeline.py</string>
